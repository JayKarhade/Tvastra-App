										<span>Sat, 9 May</span>
									</div>
									<div class="day-slots-available">
										15 Slots Available
									</div>
								</div>
								<div class="date-heading slide">
									<div class="day-date">
										<span>Sat, 10 May</span>
									</div>
									<div class="day-slots-available">
										15 Slots Available
									</div>
								</div>
								<div class="date-heading slide">
									<div class="day-date">
										<span>Sat, 11 May</span>
									</div>
									<div class="day-slots-available">
										15 Slots Available
									</div>
								</div>
								<div class="date-heading slide">
									<div class="day-date">
										<span>Sun, 12 May</span>
									</div>
									<div class="day-slots-available">
										15 Slots Available
									</div>
								</div>
							</div>
							<div class="time-slots-header-controls time-slots-header-right right">
								<i class="fas fa-chevron-right"></i>
							</div>
						</div>	


						<div class="time-slots">
							<% for(let i=0; i < slots.length; i++) { %>
							<a href="/appointment/<%= id %>?hour=<%= slots[i].split(':')[0] %>&min=<%= slots[i].split(':')[1].split(' ')[0] %>&mode=<%= slots[i].split(':')[1].split(' ')[1] %>"><span class="slot"><%= slots[i] %></span></a>
							<% } %>
						</div>
					</div>
				</div>
				<% } %>
