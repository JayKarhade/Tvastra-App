				<% for(let i = 0; i < doctors.length; i++){ %>
				<% 
					const id = doctors[i]._id;
					const start = new Date();
					start.setHours(parseInt(doctors[i].startTime.split(':')[0]));
					start.setMinutes(parseInt(doctors[i].startTime.split(':')[1]));
					const end = new Date();
					end.setHours(parseInt(doctors[i].endTime.split(':')[0]));
					end.setMinutes(parseInt(doctors[i].endTime.split(':')[1]));
					slotsWithDate = [];
					slots = [];
					function generateSlots(start, end, interval){
						let startTime = start.getTime();
						let endTime = end.getTime();
						let duration = parseInt(interval) * 60 * 1000;
						while(startTime < endTime){
							startTime += duration;
							slotsWithDate.push(new Date(startTime));
						}
					}	
						
					generateSlots(start, end, doctors[i].slotDuration);
					slotsWithDate.forEach(el => {
					slots.push(`${el.toLocaleString().split(', ')[1].split(':')[0]}:${el.toLocaleString().split(', ')[1].split(':')[1]} ${el.toLocaleString().split(', ')[1].split(':')[2].split(' ')[1]}`);
					});

				 %>
